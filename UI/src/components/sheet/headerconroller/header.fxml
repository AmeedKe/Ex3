<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="Infinity" maxWidth="Infinity" minHeight="0.0" minWidth="0.0" prefHeight="109.0" prefWidth="860.0" styleClass="header-vbox" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.headerconroller.HeaderController">
   <children>
      <HBox prefHeight="38.0" prefWidth="860.0">
         <children>
            <Label prefHeight="18.0" prefWidth="72.0" text="User Name:">
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin></Label>
            <Label fx:id="UserNameTextField" prefHeight="18.0" prefWidth="219.0" text="....">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin></Label>
            <Label fx:id="titleLabel" alignment="CENTER" prefHeight="30.0" prefWidth="224.0" text="Sheet-Cell" textFill="#FF0000">
               <font>
                  <Font name="Calibri Bold" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="200.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="ReturnToDashboardButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="138.0" text="Return To Dashboard">
               <HBox.margin>
                  <Insets right="10.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="26.0" prefWidth="860.0" spacing="10.0">
         <children>
            <Label prefHeight="26.0" prefWidth="196.0" text="Last Updated Version &amp; User Name:">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin></Label>
            <Label fx:id="lastUpdatedVersionValueLabel" prefHeight="26.0" prefWidth="251.0" text="X-Name">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox prefHeight="18.0" prefWidth="860.0">
         <children>
            <Label prefHeight="26.0" prefWidth="80.0" text="Cell ID:">
               <padding>
                  <Insets left="20.0" />
               </padding>
            </Label>
            <Label fx:id="cellIdValueLabel" alignment="CENTER" prefHeight="26.0" prefWidth="40.0" text="">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <Button fx:id="updateValueButton" disable="true" mnemonicParsing="false" onAction="#handleUpdateValueAction" prefHeight="26.0" prefWidth="120.0" styleClass="left-side-menubutton" text="Update Value">
               <HBox.margin>
                  <Insets left="10.0" right="20.0" />
               </HBox.margin>
            </Button>
            <Label alignment="CENTER" prefHeight="26.0" prefWidth="100.0" text="Original Value:">
               <HBox.margin>
                  <Insets left="-10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="originalValueTextField" disable="true" prefHeight="26.0" prefWidth="366.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="versionSelectorButton" disable="true" mnemonicParsing="false" onAction="#handleVersionSelectorAction" prefHeight="26.0" prefWidth="190.0" styleClass="left-side-menubutton" text="Version Selector">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
