<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="Infinity" maxWidth="Infinity" minHeight="0.0" minWidth="0.0" prefHeight="109.0" prefWidth="860.0" styleClass="header-vbox" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.headerconroller.HeaderController">
   <children>
      <Label fx:id="titleLabel" prefHeight="30.0" prefWidth="200.0" text="Sheet-Cell" textFill="#FF0000">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font>
      </Label>
      <HBox prefHeight="26.0" prefWidth="860.0" spacing="10.0">
         <children>
            <Button fx:id="loadFileButton" mnemonicParsing="false"  prefHeight="26.0" prefWidth="150.0" styleClass="left-side-menubutton" text="Load File">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Label prefHeight="26.0" prefWidth="150.0" text="Loaded XML File Path:" />
            <Label fx:id="loadedFilePathLabel" prefHeight="26.0" prefWidth="500.0" text=".....xml">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox prefHeight="18.0" prefWidth="860.0">
         <children>
            <Label prefHeight="26.0" prefWidth="80.0" text="Cell ID:">
               <padding>
                  <Insets left="20.0" />
               </padding>
            </Label>
            <Label fx:id="cellIdValueLabel" alignment="CENTER" prefHeight="26.0" prefWidth="40.0" text="">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <Button fx:id="updateValueButton" disable="true" mnemonicParsing="false" onAction="#handleUpdateValueAction" prefHeight="26.0" prefWidth="120.0" styleClass="left-side-menubutton" text="Update Value">
               <HBox.margin>
                  <Insets left="10.0" right="20.0" />
               </HBox.margin>
            </Button>
            <Label alignment="CENTER" prefHeight="26.0" prefWidth="100.0" text="Original Value:">
               <HBox.margin>
                  <Insets left="-10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="originalValueTextField" disable="true" prefHeight="26.0" prefWidth="265.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </TextField>
            <Label prefHeight="26.0" prefWidth="150.0" text="Last Updated Version:" />
            <Label fx:id="lastUpdatedVersionValueLabel" alignment="CENTER" prefHeight="26.0" prefWidth="50.0" text="">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="versionSelectorButton" disable="true" mnemonicParsing="false" onAction="#handleVersionSelectorAction" prefHeight="26.0" prefWidth="120.0" styleClass="left-side-menubutton" text="Version Selector">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
